class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        ArrayList<Integer> array = new ArrayList<>();
        for(int i = 0; i < m; i++){
            array.add(nums1[i]);
        } 
        for(int i = 0; i < n; i++){
            array.add(nums2[i]); 
        }
        Collections.sort(array);
        for(int i = 0; i < (m + n); i++){
            nums1[i] = array.get(i); 
        }
    }
}
